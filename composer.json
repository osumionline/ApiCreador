{
  "name": "osumionline/new",
  "description": "Aplicación para crear aplicaciones basadas en Osumi Framework.",
  "type": "project",
  "keywords": ["osumi", "framework"],
  "homepage": "https://creador.osumi.dev/",
  "license": "MIT",
  "authors": [
    {
      "name": "Iñigo Gorosabel",
      "homepage": "https://osumi.es",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.2",
    "osumionline/framework": "^9.5.5",
    "osumionline/plugin-crypt": "^3.1.0",
    "osumionline/plugin-email": "^3.1.0",
    "osumionline/plugin-file": "^3.1.0",
    "osumionline/plugin-token": "^3.0.0"
  },
  "autoload": {
    "psr-4": {
      "Osumi\\OsumiFramework\\App\\": "src/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
